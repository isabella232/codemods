{
  "name": "@prisma/codemods",
  "version": "0.0.1",
  "description": "A collection of codemods for prisma",
  "repository": "https://github.com/prisma-labs/dripip.git",
  "author": "William Luke <luke@prisma.io>",
  "license": "MIT",
  "bin": {
    "codemods": "bin/run"
  },
  "scripts": {
    "watch": "tsc -w",
    "test": "jest",
    "stable": "dripip stable",
    "preview": "dripip preview",
    "update-cli": "oclif-dev manifest && oclif-dev readme",
    "build": "tsc -b"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.2",
    "@types/debug": "4.1.5",
    "@types/jest": "26.0.15",
    "@types/jscodeshift": "0.7.1",
    "@types/node": "14.14.6",
    "dripip": "0.10.0",
    "execa": "4.1.0",
    "jest": "26.6.3",
    "prettier": "2.1.2",
    "strip-ansi": "6.0.0",
    "ts-jest": "26.4.3",
    "typescript": "4.0.5"
  },
  "dependencies": {
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/plugin-help": "3.2.0",
    "@prisma/sdk": "2.10.2",
    "debug": "2.6.9",
    "jscodeshift": "0.11.0",
    "tslib": "2.0.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "codemods",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "types": "lib/index.d.ts"
}
